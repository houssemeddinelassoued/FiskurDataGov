        <meta-data android:name="com.google.android.apps.drive.APP_ID" android:value="id=AIzaSyCRWj9ay8BoFsQnesMexcGJXRLp3RZniJc" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PackageResultActivity"
            android:label="@string/title_activity_package_result"
            android:parentActivityName="eu.fiskur.fiskurdatagov.MainActivity">
        </activity>
    </application>

</manifest>
